package com.company.message;

/**
 * @author Revature
 *
 */
public class Hello {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int number = 1;
		byte a = 2;
		char b = 'a';
		float c = 1.1f; //type casting
		double d = 1.22;
		short e = 6;
		long f = 7;
		boolean g = false;
		int h = (int)f;
		long i = number;
		int j = 'a';
		
//		System.out.println((char)j);
		int x = 200;
//		x = x + 1;
//		x += 1;
		System.out.println(x+1);
		System.out.println(x);
		
		/*
		 * operators
		 * 	unary - works with one operand
		 * 		-n : flips the sign of the number (n*-1)
		 * 	binary - works with two operands
		 * 		n+m : addition
		 * 		n-m : subtraction
		 * 		n*m : multiplication
		 * 		n/m : division
		 * 		n%m : modulus
		 * 		n+=m : addition assignment
		 * 		n-=m : subtraction assignment
		 * 		n*=m : multiplication assignment
		 * 		n/=m : division assignment
		 * 		n%=m : modulus assignment
		 * 	ternary - works with three operands
		 */

	}

}
